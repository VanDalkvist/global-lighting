namespace GlobalLighting.Core
{
	public class Size3D
	{
		public double Height { get; private set; }

		public double Width { get; private set; }

		public double Depth { get; private set; }

		public Size3D()
		{
			Height = 0;
			Width = 0;
			Depth = 0;
		}

		public Size3D(double width, double height, double depth)
		{
			Width = width;
			Height = height;
			Depth = depth;
		}
	}
}